#include<stdio.h>

void main()
{
int arr[20],i,pos,ele,temp;
printf("Enter the 10 elements:\n");
for(i=0;i<10;i++)
scanf("%d",&arr[i]);
printf("Enter the element to be inserted: ");
scanf("%d",&ele);
printf("\nEnter the position to be inserted into: ");
scanf("%d",&pos);
for(i=pos-1;i<=10;i++)
{temp=arr[i];
arr[i]=ele;
ele=temp;}
printf("Elements present:\n");
for(i=0;i<=10;i++)
printf("%d ",arr[i]);
}
